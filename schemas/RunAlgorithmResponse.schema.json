{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "success",
    "nextTrainTag"
  ],
  "properties": {
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "title": "The Message Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "success": {
      "$id": "#/properties/success",
      "type": "boolean",
      "title": "The Success Schema",
      "default": false
    },
    "nextTrainTag": {
      "$id": "#/properties/nextTrainTag",
      "type": "string",
      "title": "The Nexttraintag Schema",
      "default": "",
      "examples": [
        "station.1"
      ],
      "pattern": "^(.*)$"
    }
  }
}

